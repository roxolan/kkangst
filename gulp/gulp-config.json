{
  "paths": {
    "client": "src/client/",
    "server": "src/server/",
    "publicall": [
      "public/css/*",
      "public/img/*",
      "public/js/*",
      "public/html/*"
    ],
    "publiccss": "public/css/",
    "publicjs": "public/js/",
    "appless": [
      "src/client/**/*.less"
    ],
    "includes": [
      "lib/less/includes/",
      "node_modules/"
    ],
    "vendorcss": [
    ],
    "appjs": [
      "src/client/app/app.js",
      "src/client/app/common/services/kkangstData.service.js",
      "src/client/app/common/filters/addHtmlLineBreaks.filter.js",
      "src/client/app/common/filters/formatDistance.filter.js",
      "src/client/app/about/about.controller.js",
      "src/client/app/validation.js",
      "src/client/html/home/home.controller.js",
      "src/client/html/directives/ratingStars/ratingStars.directive.js",
      "src/client/html/directives/footerGeneric/footerGeneric.directive.js",
      "src/client/html/directives/navigation/navigation.directive.js",
      "src/client/html/directives/pageHeader/pageHeader.directive.js"
    ],
    "vendorjs": [
      "node_modules/bootstrap/dist/js/bootstrap.js"
    ],
    "angularlibs": [
      "node_modules/angular/angular.js",
      "node_modules/angular-route/angular-route.js",
      "node_modules/angular-resource/angular-resource.js",
      "node_modules/angular-sanitize/angular-sanitize.js",
      "node_modules/angular-touch/angular-touch.js",
      "node_modules/angular-ui-router/release/angular-ui-router.js"
    ],
    "appswig": [
      "src/client/app/**/*.html",
      "!**/_*.html"
    ],
    "apphtml": [
      "src/client/html/**/*.html",
      "!**/_*.html"
    ]
  }
}
